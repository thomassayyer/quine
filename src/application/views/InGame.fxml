<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TitledPane?>
<?import javafx.scene.layout.*?>

<HBox alignment="CENTER" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.162-ea" xmlns:fx="http://javafx.com/fxml/1">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
    </padding>
    <VBox>
        <TitledPane animated="false" minWidth="-Infinity" text="Cartons gagants">
           <VBox.margin>
              <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
           </VBox.margin>
            <VBox fx:id="winnersCardsPane" spacing="20.0" />
        </TitledPane>
    </VBox>
    <VBox>
        <HBox>
            <Button text="Carton plein" fx:id="cartonPlein" onAction="#onChangeTypeToCartonPlein"/>
            <Button text="[X] Ligne simple" fx:id="ligneSimple" onAction="#onChangeTypeToLignSimple"/>
        </HBox>
        <GridPane gridLinesVisible="true" fx:id="grid" />
    </VBox>
    <VBox fx:id="logoVBox" />
</HBox>
